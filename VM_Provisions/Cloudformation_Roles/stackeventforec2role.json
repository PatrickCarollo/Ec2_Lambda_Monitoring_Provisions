{
    "Version": "2012-10-17",
    "Statement": [
        
        {
            "Effect": "Allow",
            "Action": [
                "lambda:CreateFunction",
                "lambda:DeleteFunction"
            ],
            "Resource": "arn:aws:lambda:*:${accountid}:function:*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "events:DeleteRule",
                "events:PutTargets",
                "events:PutRule"
            ],
            "Resource": "arn:aws:events:*:${accountid}:rule/*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "sns:CreateTopic",
                "sns:DeleteTopic"
            ],
            "Resource": "arn:aws:sns:*:${accountid}*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:CreateBucket",
                "s3:DeleteBucket",
                "s3:GetObject"
            ],
            "Resource": "arn:aws:s3:::*/*"
        },
            "Effect": "Allow",
            "Action": [
                "iam:DeleteRole",
                "iam:CreateRole",
            ],
            "Resource": "arn:aws:iam::${accountid}*/*"
        }
    ]
}
